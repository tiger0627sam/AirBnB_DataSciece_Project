 = 'rmse',seed=42)\n","    return xgb_cv[-1:].values[0]\n","\n","grid[['train-RMSE-mean','train-RMSE-std','test-RMSE-mean','test-RMSE-std']] = grid.apply(fit,axis=1,result_type='expand')\n","\n","grid"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":238},"id":"xtk4Vmewm00h","executionInfo":{"status":"ok","timestamp":1702093984127,"user_tz":-480,"elapsed":6816,"user":{"displayName":"陳郁婷","userId":"15692740156302714785"}},"outputId":"98ab7068-418b-48bb-ccfb-05ba4aaf5ad3"},"execution_count":22,"outputs":[{"output_type":"execute_result","data":{"text/plain":["    eta  subsample  train-RMSE-mean  train-RMSE-std  test-RMSE-mean  \\\n","0  0.01        0.1      1728.495139       18.130794     1733.155475   \n","1  0.05        0.1      1492.328998       12.618686     1518.639111   \n","2  0.10        0.1      1311.649068       12.237461     1373.412459   \n","3  0.01        0.3      1712.988975       17.973389     1720.718330   \n","4  0.05        0.3      1427.132890       10.963026     1471.986378   \n","5  0.10        0.3      1204.863088       10.407438     1302.085316   \n","\n","   test-RMSE-std  \n","0      83.431448  \n","1      77.379607  \n","2      75.487852  \n","3      82.801552  \n","4      72.138613  \n","5      61.465367  "],"text/html":["\n","  <div id=\"df-dbf37d04-ba64-4989-ab30-7e5558d71788\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>eta</th>\n","      <th>subsample</th>\n","      <th>train-RMSE-mean</th>\n","      <th>train-RMSE-std</th>\n","      <th>test-RMSE-mean</th>\n","      <th>test-RMSE-std</th>\n","    </tr>\n","  <